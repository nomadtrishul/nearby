{
  "name": "nearbypetcare",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "prebuild": "npm run generate:llms || echo 'Warning: llms.json generation failed, but continuing build...'",
    "postbuild": "npx next-sitemap && node scripts/submit-indexnow.js",
    "start": "next start",
    "lint": "next lint",
    "seo:check": "ts-node --compiler-options '{\"module\":\"commonjs\"}' -r tsconfig-paths/register scripts/seo-check.js",
    "indexnow:submit": "node scripts/submit-indexnow.js",
    "generate:llms": "ts-node --compiler-options '{\"module\":\"commonjs\"}' -r tsconfig-paths/register scripts/generate-llms.ts"
  },
  "dependencies": {
    "@supabase/ssr": "^0.7.0",
    "@supabase/supabase-js": "^2.81.1",
    "@vercel/analytics": "^1.5.0",
    "@vercel/speed-insights": "^1.2.0",
    "gray-matter": "^4.0.3",
    "lucide-react": "^0.555.0",
    "next": "^16.0.2",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "recharts": "^3.5.1",
    "remark": "^15.0.1",
    "remark-gfm": "^4.0.1",
    "remark-html": "^16.0.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.15",
    "@types/node": "^24.0.0",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "autoprefixer": "^10.4.20",
    "baseline-browser-mapping": "^2.8.32",
    "eslint": "^9.39.0",
    "eslint-config-next": "^16.0.0",
    "next-sitemap": "^4.2.3",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.9.2"
  }
}
